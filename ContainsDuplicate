class Solution {
    public boolean containsDuplicate(int[] nums) {
        // [2,1,3,1]
     for(int i=0;i<nums.length;i++){//i=1 1<4,
        for(int j=i+1;j<nums.length;j++){//3<4,
            if(nums[i]==nums[j]){//1!=1,
                return true;
            }
        }
     }   
     return false;
    }
}
